{
  "name": "dsvendas",
  "version": "1.0.0",
  "description": "Sistema de Vendas DSVENDAS",
  "scripts": {
    "start": "echo 'Use npm run dev para executar frontend e backend simultaneamente'",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm start",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "keywords": [
    "vendas",
    "sistema",
    "react",
    "node"
  ],
  "author": "DSVENDAS Team",
  "license": "MIT"
}
